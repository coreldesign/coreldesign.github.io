    "politicadeprivacidade.html": {
      "settings": {
        "meta_descr": "Site Creator Description",
        "title": "Poltica de Privacidade",
        "order": 1
      },
      "components": [
        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
